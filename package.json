{
  "name": "react-template",
  "version": "1.0.0",
  "description": "A react basic template to avoid the repetitive configurations",
  "main": "index.js",
  "scripts": {
    "build": "webpack --config ./webpack.prod.js --mode production && build:css",
    "test": "webpack --config ./webpack.test.js --mode production",
    "start": "npm run watch:css && webpack-dev-server --config ./webpack.dev.js --mode development",
    "build:css": "postcss ./styles/tailwind.css -o ./styles/main.css",
    "watch:css": "postcss ./styles/tailwind.css -o ./styles/main.css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/orlandos20/react-template-boilerplate.git"
  },
  "devDependencies": {
    "@babel/core": "^7.4.3",
