    "@babel/preset-react": "^7.0.0",
    "axios": "^0.19.0",
    "axios-mock-adapter": "^1.17.0",
    "babel-loader": "^8.0.5",
    "core-js": "^3.1.4",
    "eslint": "5.15.2",
    "eslint-loader": "2.1.2",
    "eslint-plugin-import": "2.16.0",
    "eslint-plugin-react": "7.12.4",
    "eslint-import-resolver-webpack": "^0.11.1",
    "file-loader": "^4.0.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "react-hot-loader": "^4.8.4",
